{
  "name": "vening-api",
  "version": "1.0.0",
  "description": "Vending machine API",
  "main": "index.js",
  "scripts": {
    "dev": "npm run nodemon",
    "start": "export APP_ENV=production; node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config build/index.js",
    "build": "tsc",
    "docs": "apidoc -i src/ -o public/",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config --exec 'ts-node --files' src/index.ts",
    "test": "export APP_ENV=test; jest --passWithNoTests --coverage",
    "test:watch": "export APP_ENV=test; jest --watchAll",
    "coverage": "export APP_ENV=test; jest --coverage",
    "coverage:coveralls": "export APP_ENV=test; jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e": "export APP_ENV=test; jest -c jest.integration.config.js",
    "test:e2e:watch": "export APP_ENV=test; jest -c jest.integration.config.js --watchAll",
    "test:e2e:coverage": "export APP_ENV=test; jest -c jest.integration.config.js --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write ",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongoose",
    "typescript"
  ],
  "author": "gagocarrilloedgar",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/morgan": "^1.9.2",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "coveralls": "^3.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.4",
    "prettier": "^2.3.0",
    "supertest": "^6.2.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.3.0",
    "mongoose": "^5.12.10",
    "mongoose-private": "^0.1.3",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run prettier:fix",
      "npm run test"
    ]
  }
}
